<p id="notice"><%= notice %></p>

<h1>Sellers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.name %></td>
        <td><%= seller.email %></td>

        <td><%= link_to 'Show', [:signup, seller] %></td>
        <td><%= link_to 'Edit', edit_signup_seller_path(store) %></td>
        <td><%= link_to 'Destroy', [:signup, seller], method: :delete, data: { confirm: 'Are you sure?' } %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller', new_signup_seller_path %>
